{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": "India air quality",
  "width": 800,
  "height": 600,
  "projection": {"type": "equirectangular"},
  "data": {
    "url": "https://raw.githubusercontent.com/xshi0009/FIT3179/main/india-states%20(2).topojson",
    "format": {"type": "topojson", "feature": "IND_adm1"}
  },
  "transform": [
    {
      "lookup": "properties.NAME_1",
      "from": {
        "data": {
          "url": "https://github.com/xshi0009/FIT3179/blob/main/data_india_air.csv"
        },
        "key": "State",
        "fields": ["so2"]
      }
    }
  ],
  "mark": {"type": "geoshape"},
  "config": {}
}